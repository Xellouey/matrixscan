{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/manifest.json",
      "dest": "app.py"
    },
    {
      "src": "/sw.js",
      "dest": "app.py"
    },
    {
      "src": "/icon-(.*).png",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  }
}